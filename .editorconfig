# editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.{in,in-test}]
insert_final_newline = false
trim_trailing_whitespace = false


[.session]
insert_final_newline = false

[*.{js,jsx,ts,tsx,vue}]
indent_size = 4
max_line_length = 100
