{
  "name": "adventofcode",
  "version": "1.0.0",
  "description": "Advent of Code solutions and library",
  "main": "index.ts",
  "scripts": {
    "prepare": "husky install",
    "start": "ts-node -r tsconfig-paths/register index.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:lib": "eslint ./src/lib --ext .ts",
    "lint:app": "eslint ./src/app --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EldoranDev/AdventOfCode.git"
  },
  "author": "Marcel Behrmann <info@eldoran.dev>",
  "license": "mit",
  "bugs": {
    "url": "https://github.com/EldoranDev/AdventOfCode/issues"
  },
  "lint-staged": {
    "src/app/*.ts": "npm run lint:app",
    "src/lib/*.ts": "npm run lint:lib"
  },
  "homepage": "https://github.com/EldoranDev/AdventOfCode#readme",
  "devDependencies": {
    "@swc/core": "^1.2.110",
    "@types/clipboardy": "^2.0.1",
    "@types/node": "^18.11.10",
    "@types/node-fetch": "^2.5.12",
    "@types/yargs": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vitest/coverage-v8": "^2.1.6",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^12.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "clipboardy": "^2.3.0",
    "deepcopy": "^2.1.0",
    "winston": "^3.17.0",
    "yargs": "^17.7.2",
    "z3-solver": "^4.13.3"
  },
  "volta": {
    "node": "23.3.0"
  }
}
